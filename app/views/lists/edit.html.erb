<%= link_to 'My lists', user_lists_path(current_user), :class => 'btn btn-primary' %>

<%= simple_form_for [current_user, list], :html => { :class => 'form-horizontal' } do |f| %>
  <%= f.input :name %>
  <%= f.input :description %>
  
  <%= f.button :submit, :class => 'btn btn-primary' %>  

<% end %>